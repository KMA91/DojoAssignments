public class Pokedex extends AbstractClass{
	private String name;
	private int health;
	private String type;

	public String pokemonInfo(Pokemon pokemon){
		return pokemonInfo(pokemon);
	}

	@Override
	public void createPokemon(String name, int health, String type) {
		this.name = name;
		this.health = health;
		this.type = type;
	}
}
