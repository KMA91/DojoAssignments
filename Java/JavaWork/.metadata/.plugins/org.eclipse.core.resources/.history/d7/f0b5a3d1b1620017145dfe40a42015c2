
public class Gorilla extends Mammal{
	public void throw(){
		System.out.println("The gorilla is throwing things!");
		this.energyLevel -= 5;
	}
	public void eatBananas(){
		this.energyLevel += 10;
		System.out.println("The gorilla is eating bananas!");
	}
	public void climb(){
		this.energyLevel -= 10;
		System.out.println("The gorilla is climbing!");
	}
}
